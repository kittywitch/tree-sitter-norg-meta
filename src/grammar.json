{
  "name": "norg_meta",
  "rules": {
    "metadata": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "statement"
          },
          {
            "type": "SYMBOL",
            "name": "delimiter"
          }
        ]
      }
    },
    "statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword"
        },
        {
          "type": "SYMBOL",
          "name": "_separator"
        },
        {
          "type": "SYMBOL",
          "name": "_space"
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "keyword": {
      "type": "PATTERN",
      "value": "[^\\s:]+"
    },
    "value": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_text_with_trailing_modifier"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_text_till_eol"
        }
      ]
    },
    "delimiter": {
      "type": "PATTERN",
      "value": "---+\\n"
    },
    "_separator": {
      "type": "STRING",
      "value": ":"
    },
    "_space": {
      "type": "PATTERN",
      "value": "\\s+"
    },
    "_text_till_eol": {
      "type": "PATTERN",
      "value": "[^(\\~\\n)]+"
    },
    "_text_with_trailing_modifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_text_till_eol"
        },
        {
          "type": "STRING",
          "value": "~\n"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

