{
  "name": "norg_meta",
  "rules": {
    "metadata": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "statement"
          },
          {
            "type": "SYMBOL",
            "name": "delimiter"
          }
        ]
      }
    },
    "statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword"
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "keyword": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^\\s:]+"
        },
        {
          "type": "SYMBOL",
          "name": "_separator"
        }
      ]
    },
    "value": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_text_with_trailing_modifier"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_text_till_eol"
        }
      ]
    },
    "delimiter": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_delimiter"
        },
        {
          "type": "SYMBOL",
          "name": "named_delimiter"
        }
      ]
    },
    "_delimiter": {
      "type": "PATTERN",
      "value": "---+\\n"
    },
    "named_delimiter": {
      "type": "PREC",
      "value": 3,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "---+\\s*"
          },
          {
            "type": "PREC_LEFT",
            "value": 2,
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "FIELD",
                  "name": "title",
                  "content": {
                    "type": "PATTERN",
                    "value": "[^-]+"
                  }
                },
                {
                  "type": "PATTERN",
                  "value": "\\s*"
                }
              ]
            }
          },
          {
            "type": "SYMBOL",
            "name": "_delimiter"
          }
        ]
      }
    },
    "_separator": {
      "type": "STRING",
      "value": ":"
    },
    "_text_till_eol": {
      "type": "PATTERN",
      "value": "[^(\\~\\n)]+"
    },
    "_text_with_trailing_modifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_text_till_eol"
        },
        {
          "type": "STRING",
          "value": "~\n"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

