{
  "name": "norg_meta",
  "rules": {
    "metadata": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "statement"
      }
    },
    "statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword"
        },
        {
          "type": "SYMBOL",
          "name": "_separator"
        },
        {
          "type": "SYMBOL",
          "name": "_space"
        },
        {
          "type": "SYMBOL",
          "name": "description"
        }
      ]
    },
    "keyword": {
      "type": "PATTERN",
      "value": "[^\\s:]+"
    },
    "description": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_word"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_space"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_word"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_separator": {
      "type": "STRING",
      "value": ":"
    },
    "_space": {
      "type": "PATTERN",
      "value": "\\s+"
    },
    "_word": {
      "type": "PATTERN",
      "value": "\\S+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

